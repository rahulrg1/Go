# https://gqlgen.com/getting-started/

type JobListing{
  _id: ID!
  tittle: String!
  description: String!
  company: String!
  url: String!
}

type Query {
  jobs: [JobListing!]!
  job(id: ID!): JobListing!
}

type Mutation {
  createJobListing(input: CreateJobListingInput!): JobListing!
  updateJobListing(id: ID!, input: UpateJobListing!): JobListing
  deleteJoblisting(id: ID!), DeleteJobResponse!
}

