

type User {
  id: String!
  name: String!
  location: String
  age: Int!
}


input NewUser {
  name: String!
  location: String
  age: Int!
}

input UpdateUser {
  name: String!
  location: String
  age: Int!
}

type Mutation {
  createUser(input: NewUser!): User!
  updateUser(id: String!,input: UpdateUser!): User!
  deleteUser(id: String!): User!
}

type Query {
  users: [User!]!
}
